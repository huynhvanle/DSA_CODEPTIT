#include <cstring>
#include <fstream>
#include <deque>
#include <stack>
#include <queue>
#define MOD 1000000007
using namespace std;
typedef long long ll;
typedef double db;
int check(string s){
    int cnt1 = 0, cnt2 = 0;
    for(int i = 0; i < s.size(); i++){
        if(i < s.size() - 2 && s[i] == '('){
            if(s[i + 2] == ')')
                return 0;
            else cnt1++;
        }
        if(s[i] == '+' || s[i] == '-' || s[i] == '*' || s[i] == '/')
            cnt2++;
    }
    if(cnt1 > cnt2) return 0;
    return 1;
}
int main() {
    int t;
    cin >> t;
    while(t--){
        cin.ignore();
        string s;
        cin >> s;
        if(check(s)) cout << "No";
        else cout << "Yes";
        cout << endl;
    }
    return 0;
}

