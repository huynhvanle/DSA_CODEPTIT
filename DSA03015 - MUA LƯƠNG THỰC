#define MOD 1000000007
#define run() int t; cin >> t; while(t--)
#define fast() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define all(x) x.begin(), x.end()
using namespace std;
typedef long long ll;
typedef double db;

int main(){
    fast();
    run(){
        int n, s, m;
        cin >> n >> s >> m;
        if(n * 6 < m * 7)
            cout << -1;
        else{
            int lt = s * m;
            cout << (lt + n - 1) / n;
        }
        cout << endl;
    }
}
