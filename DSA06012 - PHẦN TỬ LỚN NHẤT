int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int k; cin >> k;
        int a[n];
        for(int i = 0; i < n; i++) cin >> a[i];
        sort(a, a + n, greater<int>());
        for(int i = 0; i < k; i++) cout << a[i] << " ";
        cout << endl;
    }
