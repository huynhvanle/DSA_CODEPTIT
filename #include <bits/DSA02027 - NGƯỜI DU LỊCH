#include <bits/stdc++.h>
using namespace std;

#define MOD 1000000007
#define run() int t; cin >> t; while(t--)
#define fast() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define all(x) x.begin(), x.end()
#define reset(x) memset(x, 0, sizeof(x))
#define fi first
#define se second
#define ll long long
#define db double
#define fl float

int n;
int a[20][20];
int check[20];
int res = 1e9;
int x[20];

void Try(int i, int sum){
    for(int j = 1; j <= n; j++){
        if(a[x[i - 1]][j]){
            if(!check[j]){
                x[i] = j;
                check[j] = 1;
                if(sum + a[x[i - 1]][j] < res)
                    Try(i + 1, sum + a[x[i - 1]][j]);
                check[j] = 0;
            }
            else if(i == n + 1 && j == x[1]){
                int total = sum + a[x[i - 1]][1];
                res = min (res, total);
            }
        }
    }
}

int main() {
    fast();
    cin >> n;
    for(int i = 1; i <= n; i++)
        for(int j = 1; j <= n; j++)
            cin >> a[i][j];
    x[1] = 1; check[1] = 1;
    Try(2, 0);
    cout << res;
}
