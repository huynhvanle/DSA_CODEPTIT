#include <bits/stdc++.h>
using namespace std;

#define MOD 1000000007
#define run() int t; cin >> t; while(t--)
#define fast() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define all(x) x.begin(), x.end()
#define reset(x) memset(x, 0, sizeof(x))
#define fi first
#define se second
#define ll long long
#define db double
#define fl float

int n, c;
vector<int> dsk[1000];
int color[1000];

bool dfs(int u, int par){
    color[u] = 1;
    for(int v : dsk[u]){
        if(!color[v]){
            if(dfs(v, u))
                return true;
        }else if(v != par)
            return true;
    }
    color[u] = 2;
    return false;
}
bool check(){
    for(int i = 1; i <= n; i++){
        if(!color[i]){
            if(dfs(i, 0))
                return true;
        }
    }
    return false;
}

int main(){
    run(){
        reset(color);
        reset(dsk);
        cin >> n >> c;
        for(int i = 0; i < c; i++){
            int x, y; cin >> x >> y;
            dsk[x].push_back(y);
        }
        if(check()) cout << "YES" << endl;
        else cout << "NO" << endl;
    }
}
