#include <bits/stdc++.h>
using namespace std;

#define MOD 1000000007
#define run() int t; cin >> t; while(t--)
#define fast() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define all(x) x.begin(), x.end()
#define reset(x) memset(x, 0, sizeof(x))
#define fi first
#define se second
#define ll long long
#define db double
#define fl float


int main() {
    fast();
    run(){
        string s; cin >> s;
        s += "I";
        stack<int> st;
        for(int i = 0; i < s.size(); i++){
            if(s[i] != 'I')
                st.push(i + 1);
            else{
                cout << i + 1;
                while(!st.empty()){
                    cout << st.top();
                    st.pop();
                }
            }
        }
        cout << endl;
    }
}
