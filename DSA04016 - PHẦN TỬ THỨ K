int main() {
    fast();
    run(){
        int n, m, k;
        cin >> n >> m >> k;
        int a[m + n];
        for(int i = 0; i < m + n; i++)
            cin >> a[i];
        sort(a, a + n + m);
        cout <<  a[k - 1];
        cout << endl;
    }
    return 0;
