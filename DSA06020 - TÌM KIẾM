#include <unordered_map>
#include <unordered_set>
#include <cstring>
#include <fstream>
#define MOD 1000000007
using namespace std;
typedef long long ll;
typedef double db;

int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int a[n];
        int k; cin >> k;
        for(int i = 0; i < n; i++)
            cin >> a[i];
        if(binary_search(a, a + n, k)) cout << 1;
        else cout << -1;
        cout << endl;
    }
    return 0;
}
