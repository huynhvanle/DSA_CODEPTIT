#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <vector>
#include <string>
#include <sstream>
#include <set>
#include <cctype>
#include <map>
#include <unordered_map>
#include <unordered_set>
#include <cstring>
#include <fstream>
#define MOD 1000000007
using namespace std;
typedef long long ll;
typedef double db;

int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int m; cin >> m; 
        multiset <int> s;
        for(int i = 0; i < n + m; i++){
            int x; cin >> x;
            s.insert(x);
        }
        for(int x : s) cout << x << " ";
        cout << endl;
    }
    return 0;
}
