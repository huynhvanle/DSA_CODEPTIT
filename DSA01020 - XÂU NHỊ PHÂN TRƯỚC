#define MOD 1000000007
#define test() int t; cin >> t; while(t--)
#define fast() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
typedef long long ll;
typedef double db;
int main(){
    fast();
    test(){
        string s;
        cin >> s;
        int i = s.size() - 1;
        while(i >= 0 && s[i] == '0'){
            s[i] = '1';
            i--;
        }
        if(i >= 0){
            s[i] = '0';
            cout << s;
        }else{
            for(int j = 0; j < s.size(); j++)
                cout << 1;
        }
        cout << endl;
    }
    return 0;
}
