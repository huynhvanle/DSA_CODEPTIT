#include <bits/stdc++.h>
#define MOD 1000000007
using namespace std;
typedef long long ll;
typedef double db;
int main(){
    int n;
    cin >> n;
    multiset<int> a;
    for(int i = 0; i < n; i++){
        int x; cin >> x;
        a.insert(x);
        cout << "Buoc " << i << ": ";
        for(int k : a)
            cout << k << " ";
        cout << endl;
    }
    return 0;
}
